{
  "name": "getenv2",
  "version": "1.0.0",
  "repository": "git://github.com/sauramirez/getenv2",
  "description": "Retrieve and validate environment variables",
  "keywords": [
    "env",
    "environment",
    "configuration",
    "config",
    "12factor"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "document": "jsdoc -c ./jsdoc.json lib",
    "document-markdown": "jsdoc2md -f 'lib/**/*.js' > doc/README.md",
    "test": "lab -c -a code -r console -o stdout -r lab-markdown-reporter -o doc/coverage.md --coverage-path lib -L"
  },
  "author": "Sau Ramirez",
  "license": "MIT",
  "devDependencies": {
    "code": "3.x.x",
    "docdash": "0.4.x",
    "eslint": "4.x.x",
    "eslint-config-hapi": "10.x.x",
    "eslint-plugin-hapi": "4.x.x",
    "jsdoc": "3.x.x",
    "jsdoc-to-markdown": "3.x.x",
    "lab": "14.x.x",
    "lab-markdown-reporter": "1.0.x"
  },
  "dependencies": {
    "joi": "10.6.x"
  }
}
