{
  "name": "getenv2",
  "version": "3.0.0",
  "repository": "git://github.com/sauramirez/getenv2",
  "description": "Retrieve and validate environment variables",
  "keywords": [
    "env",
    "environment",
    "configuration",
    "config",
    "getenv",
    "process.env",
    "12factor"
  ],
  "engines": {
    "node": ">=4.0.0"
  },
  "main": "lib/index.js",
  "scripts": {
    "document": "jsdoc -c ./jsdoc.json lib",
    "document-markdown": "jsdoc2md -f 'lib/**/*.js' > doc/README.md",
    "test": "lab -c -a @hapi/code -r console -o stdout -r lab-markdown-reporter -o doc/coverage.md --coverage-path lib -L"
  },
  "author": "Sau Ramirez",
  "license": "MIT",
  "devDependencies": {
    "@bekindsoft/eslint-config-bekind": "^0.2.1",
    "@hapi/code": "^8.0.3",
    "@hapi/eslint-plugin": "^5.0.0",
    "@hapi/lab": "^24.1.1",
    "docdash": "1.x.x",
    "eslint": "7.x.x",
    "jsdoc": "3.x.x",
    "jsdoc-to-markdown": "7.x.x",
    "lab-markdown-reporter": "1.0.x"
  },
  "dependencies": {
    "joi": "17.x.x"
  }
}
